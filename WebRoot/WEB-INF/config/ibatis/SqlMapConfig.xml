	<sqlMap resource="com/cbai/model/common/ibatis/sqlmap/common/*_SqlMap.xml"/>
	
	<sqlMap resource="com/cbai/model/rongyin/ibatis/sqlmap/sys/*_SqlMap.xml"/>
	<!-- @admin end -->

	<!--
	<sqlMap resource="com/cbai/common/wx/framework/dto/*_SqlMap.xml"/>
	--> 
	
	<!-- @front start --> 
	<sqlMap resource="com/cbai/model/rongyin/ibatis/sqlmap/pc/*_SqlMap.xml"/>
    <!-- @front end --> 
    
</sqlMapConfig>
